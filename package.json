        "eslint": "^8.27.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-import-resolver-typescript": "^3.5.2",
        "eslint-plugin-import": "^2.26.0",
        "lerna": "^6.4.1",
        "prettier": "^2.8.1",
        "prettier-eslint": "^15.0.1",
        "rollup": "3.7.4",
        "rollup-plugin-copy": "3.4.0",
        "typescript": "4.9.4"
    }
